{
  "author": "Mohammad Shahmizan",
  "name": "ICT 2202 Assignment 1 Browser Forensics",
  "version": "0.4",
  "manifest_version": 3,
  "description": "Chrome extension to aid Browser Forensics for all!",
  "homepage_url": "https://github.com/2000726/DigiFor",
  
  "icons": {
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  
  "action": {
    "default_popup": "popup.html"
  },

  "host_permissions": [ "*://*/*" ],
  
  "permissions": [ "downloads", "history", "bookmarks", "cookies", "tabs", "topSites" ],
  "update_url": "https://clients2.google.com/service/update2/crx"

}
